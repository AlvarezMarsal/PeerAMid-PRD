@using PeerAMid.Support
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width" />
    <title>PeerAMid</title>
    <link rel="shortcut icon" href="https://www.alvarezandmarsal.com/sites/default/themes/custom/am/favicon.ico" type="image/vnd.microsoft.icon" />
    <link href="https://fonts.googleapis.com/css?family=Fira+Sans" rel="stylesheet">
    <!--	css-->
    <link rel="stylesheet" type="text/css" href="~/PeerAmidContent/plugins/fontawesome/css/all.min.css">
    <link rel="stylesheet" type="text/css" href="~/PeerAmidContent/plugins/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/PeerAmidContent/css/uikit.css">
    <link rel="stylesheet" type="text/css" href="~/PeerAmidContent/css/default.css">
    <link rel="stylesheet" type="text/css" href="~/PeerAmidContent/css/Chevron_v1.css">
    <link rel="stylesheet" type="text/css" href="~/PeerAmidContent/plugins/multiselect/bootstrap-multiselect.css">

    <script src="~/PeerAmidContent/js/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/PeerAmidContent/js/popper.min.js"></script>
    <script src="~/PeerAmidContent/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/PeerAmidContent/js/master.js"></script>
    <script src="~/PeerAmidContent/js/f4c09a9637.js"></script>
